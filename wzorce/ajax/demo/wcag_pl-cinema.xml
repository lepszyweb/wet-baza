      <dcst:Subtitle SpotNumber="1" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:00:00" TimeOut="00:00:10:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">W-C-A-G, WCAG, jestem fajnym DJ-em,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">SHARKY, SH-SH-SHARKY</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="2" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:11:00" TimeOut="00:00:17:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Hej, oszaleję z tym fajnym bitem, mam lepszy rytm od czasu,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">gdy zainstalowałeś nowy program kwantyfikujący.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="3" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:17:00" TimeOut="00:00:21:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Tańczę elektryczne boogie. Chcesz to zobaczyć David?</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="4" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:21:00" TimeOut="00:00:27:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Nie Sharky, musimy się zająć tą piosenką, możesz tańczyć,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">kiedy ja śpiewam, ale nie wyciągaj się znowu z gniazdka.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="5" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:27:00" TimeOut="00:00:31:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Mamy zupełnie nową specyfikację,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">która wspódziała z narzędziem weryfikacji,</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="6" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:31:00" TimeOut="00:00:34:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">sprawi, że dostępność będzie możliwa do sprawdzenia</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="7" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:34:00" TimeOut="00:00:35:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">WCAG, WCAG</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="8" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:36:00" TimeOut="00:00:41:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">sprawi, że sieć będzie zrozumiała,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">a jej postrzeganie stanie się namacalne,</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="9" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:41:00" TimeOut="00:00:44:12">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Interoperacyjność Sieci będzie nie do zatrzymania.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="10" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:44:13" TimeOut="00:00:47:12">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">WCAG, WCAG</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="11" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:47:13" TimeOut="00:00:52:12">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać ci WCAG,</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="12" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:52:13" TimeOut="00:00:58:12">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać Ci, jaki może być ten świat</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="13" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:00:58:13" TimeOut="00:01:02:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać ci WCAG.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="14" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:02:00" TimeOut="00:01:07:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Nie jest to ciężkie zadanie</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">otworzyć swój kod i pisać według WCAG, WCAG</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="15" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:07:00" TimeOut="00:01:14:00">
        <dcst:Text Vposition="20" Valign="bottom" Halign="center" Direction="ltr">Hej, David, widziałem w TV faceta, jak mówił, że jest</dcst:Text>
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">najszybszym raperem na świecie, ale wysadzam go</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">w powietrze, rozwalam go, oglądam to...</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="16" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:14:00" TimeOut="00:01:20:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">[Sharky rapuje 250 słów na minutę]</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="17" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:20:00" TimeOut="00:01:29:00">
        <dcst:Text Vposition="20" Valign="bottom" Halign="center" Direction="ltr">Jestem świetnym DJ-em, Sharky, WCAG…</dcst:Text>
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Lubię muzykę maniaków techno.</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">I jak, Dawidzie, jestem teraz fajnym raperem?</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="18" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:29:00" TimeOut="00:01:31:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">[David] Sharky, nie mogłem zrozumieć ani słowa,które wymówiłeś.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="19" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:31:00" TimeOut="00:01:34:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">[Sharky] Większość moich niewidomych przyjaciół</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">rozumie wszystko, gdy mówię w takim tempie.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="20" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:34:00" TimeOut="00:01:39:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">[powoli] Chcesz... żebym mówił... naprawdę...</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">powoli... dla... ciebie... David?</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="21" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:39:00" TimeOut="00:01:43:18">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">[David] To bardzo zabawne, śpiewajmy ten werset razem.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="22" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:43:18" TimeOut="00:01:48:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Zrobimy z Sieci lepsze miejsce</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">dla każdej umiejętności i różnych ludzi</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="23" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:48:00" TimeOut="00:01:53:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Nawet jeśli surfujesz z kosmosu, masz WCAG, WCAG</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="24" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:53:13" TimeOut="00:01:59:12">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać ci WCAG,</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="25" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:01:59:13" TimeOut="00:02:04:12">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać Ci, jaki może być ten świat</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="26" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:04:13" TimeOut="00:02:08:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać ci WCAG.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="27" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:08:00" TimeOut="00:02:12:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Nie jest to ciężkie zadanie otworzyć swój kod</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">i pisać według WCAG, WCAG.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="28" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:12:00" TimeOut="00:02:15:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Więc pozwól mi powiedzieć,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">czym jest WCAG i czym WCAG nie jest</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="29" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:15:00" TimeOut="00:02:18:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">WCAG to zestaw narzędziowy,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">który zawiera zestaw reguł</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="30" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:18:00" TimeOut="00:02:23:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Pomaga ci ożywiać twoją witrynę dla ludzi</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">z innymi zdolnościami i wrażliwościami.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="31" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:23:02" TimeOut="00:02:26:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Kiedy więc trafią do Ciebie w Sieci,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">mogą się dobrze bawić.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="32" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:26:00" TimeOut="00:02:28:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Nieważne, że są niewidomi, albo że muszą migać...</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="33" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:28:02" TimeOut="00:02:31:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">albo że ich ręce zaczynają się trząść przy każdym ruchu</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="34" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:31:02" TimeOut="00:02:33:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Albo jeśli ich umysły pomylą, co chcą zrobić</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="35" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:33:02" TimeOut="00:02:39:00">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Mogą łatwo zrozumieć, o czym mówisz,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">jeśli będziesz przestrzegać zasad, które przedstawiamy</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="36" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:39:02" TimeOut="00:02:44:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Zasada 1: Treść musi być postrzegalna</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="37" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:44:02" TimeOut="00:02:49:00">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Zasada 2: Elementy interfejsu muszą być funkcjonalne</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="38" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:49:02" TimeOut="00:02:54:02">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Zasada 3: Treść i kontrolki muszą być zrozumiałe</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="39" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:02:54:03" TimeOut="00:03:02:02">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Zasada 4: Treść musi być rzetelnie kodowana, aby działać</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">z obecnymi i przyszłymi technologiami</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="40" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:05:02" TimeOut="00:03:15:03">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać ci WCAG,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać ci, jaki może być ten świat</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="41" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:15:03" TimeOut="00:03:20:02">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Chcemy pokazać ci WCAG.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="42" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:20:03" TimeOut="00:03:26:02">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Nie jest to ciężkie zadanie otworzyć swój kod</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">i pisać według WCAG, WCAG</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="43" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:26:03" TimeOut="00:03:30:02">
        <dcst:Text Vposition="20" Valign="bottom" Halign="center" Direction="ltr">Sharky: Hej David, twoja muzyka jest o wiele bardziej zabawna,</dcst:Text>
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">odkąd wyniosłeś się z przedmieść</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">i przebywasz w centrum  miasta na rogach ulic.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="44" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:30:03" TimeOut="00:03:34:02">
        <dcst:Text Vposition="20" Valign="bottom" Halign="center" Direction="ltr">Lubię temat drugiej piosenki,</dcst:Text>
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">ale brzmiała trochę jak niepodłączona,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">a ja nigdy nie szalałem za wyłączonym dźwiękiem,</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="45" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:34:03" TimeOut="00:03:39:02">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Lubię muzykę techno-maniaków, gdzie komputery robią wszystko,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">a ludzie nie grają na żadnych instrumentach.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="46" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:39:03" TimeOut="00:03:41:02">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">Myślę, że jest to o wiele bardziej naturalne,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">gdy muzyka jest odtwarzana, kup oryginalne komputery.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="47" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:41:03" TimeOut="00:03:42:02">
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">Kiedy ludzie grają, brzmi to tak bezemocyjnie i fałszywie.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="48" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:42:03" TimeOut="00:03:45:02">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">David: Będziemy to robić tylko dlatego,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">że jesteście kupą zer i zer, jak wielkoludy.</dcst:Text>
      </dcst:Subtitle>
      <dcst:Subtitle SpotNumber="49" FadeUpTime="00:00:00:00" FadeDownTime="00:00:00:00" TimeIn="00:03:45:03" TimeOut="00:03:50:03">
        <dcst:Text Vposition="14" Valign="bottom" Halign="center" Direction="ltr">[Sharky] David, to było wredne z twojej strony, że to powiedziałeś,</dcst:Text>
        <dcst:Text Vposition="8" Valign="bottom" Halign="center" Direction="ltr">wiesz, że zawsze byłem bardzo wrażliwy na moje binarne kodowanie strukturalne.</dcst:Text>
      </dcst:Subtitle>
